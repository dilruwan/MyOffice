{
    "ip": "192.168.10.11",
    "memory": 2048,
    "cpus": 1,
    "hostname": "myoffice",
    "name": "myoffice",
    "provider": "virtualbox",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": [
        "~/.ssh/id_rsa"
    ],
    "folders": [
        {
            "map": "~/Development/VueProject/server",
            "to": "/home/vagrant/code"
        }
    ],
    "sites": [
        {
            "map": "myoffice.dev",
            "to": "/home/vagrant/code/public"
        }
    ],
    "databases": [
        "myoffice"
    ]
}
